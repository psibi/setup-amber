{
  "name": "setup-amber",
  "version": "0.0.0",
  "private": true,
  "description": "Setup amber and cache it",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint",
    "lint-fix": "eslint --fix src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "vitest run",
    "all": "npm run build && npm run format && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psibi/setup-amber.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/tool-cache": "^2.0.1",
    "@octokit/rest": "^21.0.2",
    "fs": "^0.0.1-security",
    "neverthrow": "^8.0.0",
    "path": "^0.12.7",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@types/node": "^22.7.7",
    "@typescript-eslint/parser": "^8.10.0",
    "@vercel/ncc": "^0.38.2",
    "eslint": "^9.13.0",
    "eslint-plugin-github": "^5.0.2",
    "js-yaml": "^4.1.0",
    "prettier": "3.3.3",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}
